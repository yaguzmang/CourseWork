INICIO:
        cero := 0
        uno := 1
	dos := 2
	muno := uno - dos
        readInt(n)
        if n <> muno then goto CICLO
        writeInt(cero)
        goto FIN
CICLO:
        cont := cont + uno
	suma := suma + n
	readInt(n)
	if n <> muno then goto CICLO
	promedio := suma/cont
        writeInt(promedio)
        goto FIN
FIN:
        halt
equ cero M[0]
equ uno M[1]
equ suma M[2]
equ n M[3]
equ cont M[4]
equ muno M[5]
equ dos M[6]
equ promedio M[7]