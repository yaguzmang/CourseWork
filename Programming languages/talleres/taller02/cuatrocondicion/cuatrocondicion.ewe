INICIO:
        cero := 0
	uno := 1
	readInt(a)
	readInt(b)
	readInt(c)
	readInt(d)
        bmc := b-c
        cmd := c-d
	if a < bmc then goto COND2
	writeInt(cero)
	goto FIN
COND2:
        if b < cmd then goto TCOND2
	writeInt(cero)
	goto FIN
TCOND2:
        writeInt(uno)
	goto FIN
FIN:
        halt
equ cero M[0]
equ uno M[1]
equ a M[2]
equ b M[3]
equ c M[4]
equ d M[5]
equ bmc M[6]
equ cmd M[7]